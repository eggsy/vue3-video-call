{
   "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "preserve",
      "types": ["vite/client", "unplugin-icons/types/vue3"],
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "lib": ["esnext", "dom"],
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictFunctionTypes": true,
      "alwaysStrict": false,
      "noImplicitReturns": true
   },
   "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "demo/**/*"],
   "exclude": ["node_modules"]
}
